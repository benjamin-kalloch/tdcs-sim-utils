FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      ElPot;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 2 -3 0 0 -1 0];

internalField   uniform 0;

boundaryField
{
	patch0
	{
        type            fixedValue;
        value           uniform -5;
    }
	
    patch1
	{
        type            fixedValue;
        value           uniform -5;
    }
    
    patch2
	{
        type            fixedValue;
        value           uniform -5;
    }

    patch3
	{
        type            fixedValue;
        value           uniform -5;
    }

    patch4
	{
        type            fixedValue;
        value           uniform 5;
    }

    defaultFaces
    {
        type            zeroGradient;
    }
}


